.block
  .block_head
    .bheadl
    .bheadr
    %h2 Settings
    %ul
      %li= @account.subdomain
  .block_content
    - if current_user == @account.owner
      %h3 Account Administration
      = link_to 'add user', new_subdomain_user_path(@account)

    %ul
      - @account.users.each do |user|
        %li= user.try(:to_s)
